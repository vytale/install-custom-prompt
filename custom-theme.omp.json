{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#29315A",
          "foreground": "#3EC669",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "style": "mixed"
          },
          "style": "diamond",
          "template": "\ue5ff {{ .Path }}",
          "type": "path"
        },
        {
          "background": "#29315A",
          "foreground": "#43CCEA",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "style": "mixed"
          },
          "style": "diamond",
          "template": "{{ if .Root }}<#ff0000>{{ else }}<#43CCEA>{{ end }}{{ .UserName }}</>{{ if .SSHSession }} \ueba9 {{ else }}<transparent> / </>{{ end }}{{ .HostName }}",
          "type": "session"
        },
        {
          "background": "#29315A",
          "foreground": "#e28024",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "leading_diamond": " \ue0b6",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        },
        {
          "foreground": "#C94A16",
          "style": "plain",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uebfb",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
     {
      "alignment": "right",
      "segments": [
        {
          "type": "docker",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "background": "#29315A",
          "foreground": "#5fb157",
          "template": "\uf21f {{ .Context }}"
        },
        {
          "background": "#29315A",
          "foreground": "#5fb157",
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": true,
            "fetch_version": true,
            "npm_icon": "<#FE4A49>\ue71e npm</> ",
            "yarn_icon":"<#44FFD2>\uf487 yarn</> "
          },
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}\ue718 {{ .Full }}",
          "type": "node"
        },
        {
          "background": "#29315A",
          "foreground": "#FED142",
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": false
          },
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue73c {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "python"
        },
        {
          "background": "#29315A",
          "foreground": "#da7a0d",
          "properties": {
            "display_mode": "context"
          },
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "java"
        },
        {
          "background": "#29315A",
          "foreground": "#94ffa2",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "<#ffffff>\uf017</> {{ .FormattedMs }}s",
          "type": "executiontime"
        },
        {
          "background": "#29315A",
          "foreground": "#94ffa2",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " <#ffffff>\uefc5</> {{ round .PhysicalPercentUsed .Precision }}% ({{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB)",
          "type": "sysinfo"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#dd1e1e",
          "style": "plain",
          "template": "\u279c ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
